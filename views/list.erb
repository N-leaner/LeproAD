<h3>List of Users:</h3>
<div>
	<table class="table table-striped">
		<thead>
			<tr>			
			<th>Id</th>
			<th>User</th>
			<th>Password</th>
			<th>Registrations</th>
			</tr>
		</thead>
		<tbody>
			<% @users.each do |user| %>
				<tr>
					<td><%= user.id %></td>
					<td><%= user.name %></td>
					<td><%= user.password %></td>
					<td><%= user.created_at %></td>
				</tr>
			<%end%>			
		</tbody>	
	</table>
</div>